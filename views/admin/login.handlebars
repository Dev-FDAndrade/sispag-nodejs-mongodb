<form class="form-signin text-center" method="POST" action="/admin/login">
    <img class="mb-4" src="/images/sispag.png" alt="SisPag - FDAndrade" width="72" height="72">
    <h1 class="h3 mb-3 font-weight-normal">Área Restrita</h1>
    {{> _msg}}
    <div class="form-group text-left">
        <label>CPF do Usuário</label>
        <input type="text" name="cpf" class="form-control" placeholder="Digite seu CPF">
    </div>
    <div class="form-group text-left">
        <label>Senha</label>
        <input type="password" name="senha" class="form-control" placeholder="Digite sua Senha">
    </div>
    <button class="btn btn-lg btn-primary btn-block" type="submit">Acessar</button>
    <p class="text-center">Esqueceu a senha?</p>
</form>